<h1>People#index</h1>
<p>Find me in app/views/people/index.html.erb</p>

<table class="table">
  <thead>
    <tr>
      <th>id</th>
      <th>name</th>
      <th>last_name</th>
      <th>email</th>
      <th>age</th>
      <th>gender</th>
    </tr>
  </thead>
  <tbody>
    <% @people.each do  |person| %>
      <tr>
        <td><%= person.id %></td> 
        <td><%= person.name %></td> 
        <td><%= person.last_name %></td> 
        <td><%= person.email %></td> 
        <td><%= person.age %></td> 
        <td><%= person.gender ? "male" : "female" %></td> 
      </tr>
    <% end %>
  </tbody>
</table>
